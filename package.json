{
  "name": "@hypernym/svelte-markdown-workspace",
  "version": "0.0.0",
  "author": "Hypernym Studio",
  "private": true,
  "type": "module",
  "scripts": {
    "build:sm": "pnpm --filter @hypernym/svelte-markdown build",
    "copy:sm": "pnpm --filter @hypernym/svelte-markdown copy",
    "publish:sm": "pnpm copy:sm && pnpm --filter @hypernym/svelte-markdown publish:sm",
    "lint:sm": "pnpm --filter @hypernym/svelte-markdown lint",
    "format:sm": "pnpm --filter @hypernym/svelte-markdown format",
    "dev:play": "pnpm --filter playground-sveltekit dev",
    "build:play": "pnpm --filter playground-sveltekit build",
    "preview:play": "pnpm --filter playground-sveltekit preview",
    "sync:play": "pnpm --filter playground-sveltekit sync",
    "lint:play": "pnpm --filter playground-sveltekit lint",
    "format:play": "pnpm --filter playground-sveltekit format",
    "update:all": "taze -r --interactive"
  },
  "packageManager": "pnpm@10.6.1",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=10.0.0"
  },
  "devDependencies": {
    "@hypernym/bundler": "^0.14.3",
    "@hypernym/eslint-config": "^3.5.9",
    "@hypernym/prettier-config": "^3.2.3",
    "@hypernym/tsconfig": "^2.6.0",
    "@sveltejs/kit": "^2.18.0",
    "@types/node": "^22.13.9",
    "eslint": "^9.21.0",
    "prettier": "^3.5.3",
    "svelte": "^5.22.5",
    "typescript": "^5.8.2"
  }
}
